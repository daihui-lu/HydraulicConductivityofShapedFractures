6
(
    inlet
    {
        type            patch;
        nFaces          1600;
        startFace       470400;
    }
    outlet
    {
        type            patch;
        nFaces          1600;
        startFace       472000;
    }
    lowerWall
    {
        type            wall;
        inGroups        1(wall);
        nFaces          4000;
        startFace       473600;
    }
    upperWall
    {
        type            symmetryPlane;
        inGroups        1(symmetryPlane);
        nFaces          4000;
        startFace       477600;
    }
    frontWall
    {
        type            cyclic;
        inGroups        1(cyclic);
        nFaces          4000;
        startFace       481600;
        matchTolerance  0.0001;
        transform       unknown;
        neighbourPatch  backWall;
    }
    backWall
    {
        type            cyclic;
        inGroups        1(cyclic);
        nFaces          4000;
        startFace       485600;
        matchTolerance  0.0001;
        transform       unknown;
