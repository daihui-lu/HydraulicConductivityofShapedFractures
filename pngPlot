set term png
set out "residuals.png"
set logscale y
set format y "10^{%L}"

set ylabel 'Residual'
set xlabel 'Iteration'
plot "< cat log | grep 'Solving for Ux' | cut -d' ' -f9 | tr -d ','" title 'u' with lines,\
"< cat log | grep 'Solving for Uz' | cut -d' ' -f9 | tr -d ','" title 'v' with lines,\
"< cat log | grep 'Solving for p' | cut -d' ' -f9 | tr -d ','" title "p" with lines
set out
