FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (1 0 0);

boundaryField
{
   wall
   {
        type        porousInterfaceBJS;
        slipCoeff   0.02;
	alpha	    0;
        value       $internalField;
    }


    inlet
    {
        type            fixedProfile;
        value           uniform (1 0 0);

        profile         polynomial
        (
            ((1.5 0 0)        (0 0 0))
            ((-1.5 0 0)    (2 0 0))
        );
        direction       (0 0 1);
        origin          0;
    }

    outlet
    {
        type            pressureInletOutletVelocity;
        value           uniform (0 0 0);

    }


    #includeEtc "caseDicts/setConstraintTypes"
}

// ************************************************************************* //
