FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      sample;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
#include "../parameters.H"

z0  #calc "1.0 + 50*$alpha";
z1 #calc "-1.0 - 50*$alpha";	

type sets;
libs            ("libsampling.so");

interpolationScheme cellPointFace;

setFormat       raw;

sets
(
    data
    {
        type    lineFace;
        axis    z;
        start   (50 0 $z0);
        end     (50 0 $z1);
        nPoints 100;
    }
);

fields          (U);

