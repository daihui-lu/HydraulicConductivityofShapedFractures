FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      sample;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

type sets;
libs            ("libsampling.so");

interpolationScheme cell;

setFormat       raw;

sets
(
    data
    {
        type    lineFace;
        axis    z;
        start   (50 0 -1);
        end     (50 0 1);
        nPoints 100;
    }
);

fields          (U);

